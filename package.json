{
  "name": "tp-sd-gestao-filmes",
  "version": "1.0.0",
  "description": "API REST para gestão de catálogo de filmes - Trabalho Prático Sistemas Distribuídos",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:make": "knex seed:make",
    "seed:run": "knex seed:run",
    "test": "mocha --timeout 30000 --require test/setup.js",
    "test:watch": "mocha --watch --timeout 30000 --require test/setup.js",
    "test:migrate": "knex migrate:latest --env test"
  },
  "keywords": [
    "api",
    "rest",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": "Nuno Carneiro (34106), Jorge Cunha (34128)",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "knex": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "supertest": "^6.3.3"
  }
}

